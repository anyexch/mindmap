{
  "manifest_version": 2,
  "name": "Mind Map",
  "version": "1.0",
  "description": "Open a new tab to display today's browsing history.",
  "permissions": [
    "history",
    "tabs",
    "storage",
	"webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Mind Map"
  },
  "icons": {
    "48": "icon.png"
  }
}
